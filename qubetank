void setup() {

  pinMode(moteur_gauche_enable, OUTPUT);
  pinMode(moteur_gauche_noir, OUTPUT);
  pinMode(moteur_gauche_rouge, OUTPUT);
  pinMode(moteur_droit_enable, OUTPUT);
  pinMode(moteur_droit_noir, OUTPUT);
  pinMode(moteur_droit_rouge, OUTPUT);
  
  pinMode(2, INPUT); 
  digitalWrite(2, HIGH); 
  
  pinMode(3, INPUT); 
  digitalWrite(3, HIGH);
  
  pinMode(13, OUTPUT); 
  digitalWrite(13, LOW);

  attachInterrupt(1, compteur_gauche, CHANGE);
  attachInterrupt(0, compteur_droit , CHANGE);
  
  Serial.begin(9600);
  
  chaine[0]='T';
  
}



void loop() {
 
   if(Serial.available() > 0)
   {
     //on lit sur le port serie une chaine de 40 caracteres max <- sauf si un <cr> (13) vient clore la ligne et stock le tout dans la variable chaine
     index = Serial.readBytesUntil (13,chaine,40); 
     chaine[index] = '\0'; // on clos la chaine proprement
   }  
   
   if(chaine[0]!='T'){

     for(int i=0; i<=index;i++) {       
       switch (chaine[i]) {
        case 'A':
          avance();
          Serial.write('+'); 
        break;
        case 'R':
          recul(); 
          Serial.write('+');
        break;
        case 'G':
          gauche();
          Serial.write('+');          
        break;
        case 'D':
          droite(); 
          Serial.write('+');          
        break;
        case '\0':
          Serial.write('F');
          for (int j=0; j<41;j++)
          {
            chaine[j]='\0';
          } 
          index=0;
          chaine[0]='T'; 
          delay(500);
          startPlayback(sample, sizeof(sample));
          minirecul();
          digitalWrite(moteur_gauche_noir, LOW);
          digitalWrite(moteur_gauche_rouge, LOW); 
          digitalWrite(moteur_droit_noir, LOW);
          digitalWrite(moteur_droit_rouge, LOW);
          goto fin;
       }
   }

  } 
   fin:;
}



void compteur_gauche() {
  cpt_gauche++;
  //Serial.println("G");
  
} 

void compteur_droit() {
  cpt_droit++;
  // Serial.println("D");
   
} 


void controle_moteur() {

  cpt_gauche=0;
  cpt_droit=0;
  while( cpt_gauche<longueur+1 && cpt_droit<longueur+1) {
    if(cpt_gauche>=longueur){
      analogWrite(moteur_gauche_enable, 0);
    }
    if(cpt_droit>=longueur){
       analogWrite(moteur_droit_enable, 0);
    }
    delay(5);
   // Serial.print("gauche : ");Serial.println(cpt_gauche);
    //Serial.print("droit  : ");Serial.println(cpt_droit);
  }  
  //delay(100);
  return;
  
} 



void avance() {
  analogWrite(moteur_gauche_enable, vitesse); 
  analogWrite(moteur_droit_enable, vitesse);
  
  digitalWrite(moteur_gauche_noir, HIGH);
  digitalWrite(moteur_gauche_rouge, LOW); 
  digitalWrite(moteur_droit_noir, HIGH);
  digitalWrite(moteur_droit_rouge, LOW);
  delay(100);
  longueur=20;
  controle_moteur();

  
} 
 
void recul() {
  analogWrite(moteur_gauche_enable, vitesse); 
  analogWrite(moteur_droit_enable, vitesse);

  digitalWrite(moteur_gauche_rouge, HIGH);
  digitalWrite(moteur_gauche_noir, LOW); 
  digitalWrite(moteur_droit_rouge, HIGH);
  digitalWrite(moteur_droit_noir, LOW); 
  delay(100);
  longueur=20;
  controle_moteur();
  
} 


void droite() {
  analogWrite(moteur_gauche_enable, vitesse); 
  analogWrite(moteur_droit_enable, vitesse);

  digitalWrite(moteur_gauche_noir, HIGH);
  digitalWrite(moteur_gauche_rouge, LOW); 
  digitalWrite(moteur_droit_rouge, HIGH);
  digitalWrite(moteur_droit_noir, LOW); 
  delay(100);
  longueur=16;
  controle_moteur(); 

} 


void gauche() {
  analogWrite(moteur_gauche_enable, vitesse); 
  analogWrite(moteur_droit_enable, vitesse);

  digitalWrite(moteur_gauche_noir, LOW);
  digitalWrite(moteur_gauche_rouge, HIGH); 
  digitalWrite(moteur_droit_rouge, LOW);
  digitalWrite(moteur_droit_noir, HIGH);
  delay(100); 
  longueur=16;
  controle_moteur(); 
}


void minirecul() {
  analogWrite(moteur_gauche_enable, 255); 
  analogWrite(moteur_droit_enable, 255);

  digitalWrite(moteur_gauche_rouge, HIGH);
  digitalWrite(moteur_gauche_noir, LOW); 
  digitalWrite(moteur_droit_rouge, HIGH);
  digitalWrite(moteur_droit_noir, LOW); 
  delay(80);
  analogWrite(moteur_gauche_enable, 0); 
  analogWrite(moteur_droit_enable, 0);
}
